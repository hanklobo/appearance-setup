{
    "name": "hanklobo/appearance-setup",
    "description": "A Laravel package for customizing application appearance",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Hank Lobo",
            "email": "hanklobo@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^11.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "HankLobo\\AppearanceSetup\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "HankLobo\\AppearanceSetup\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "HankLobo\\AppearanceSetup\\AppearanceSetupServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}